{
  "wsagger": "0.0.3",

  "name": "test socket.io-connect (on 'connectedTo')",

  "parameters": [
    {
      "name": "host",
      "defaultValue": "localhost"
    },

    {
      "name": "port",
      "defaultValue": 10002
    },

    {
      "name": "path",
      "defaultValue": "/"
    }
  ],


  "data": [
  
    {
      "action": "socket_io.connect",           
      "data": { 
        "url": "http://{{host}}:{{port}}{{path}}"
